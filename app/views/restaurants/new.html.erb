<%= link_to 'Back to restaurants', restaurants_path%>
<hr>
<%if flash[:notice].present? %>
<div class= 'alert alert-success'>
<%= flash[:notice] %>
</div>
<%end %>

<%= simple_form_for(@restaurant) do |f| %>
  <%= f.input :name %>
  <%= f.input :adress %>
  <%= f.input :rating %>
  <%= f.input :category %>
  <%= f.submit %>
<% end %>
